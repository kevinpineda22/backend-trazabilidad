{
  "version": 2,
  "functions": {
    "app.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "src/**"
    }
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
